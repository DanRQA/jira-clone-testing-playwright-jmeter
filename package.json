{
  "name": "jira-clone",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --pretty --noEmit",
    "lint": "npx eslint --ext ts,tsx src",
